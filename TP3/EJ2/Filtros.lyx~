\begin_layout Subsubsection*
Low pass con gyrator:
\end_layout

\begin_layout Standard
Con las siguientes ecuaciones se obtuvo la función transferencia:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
V^{-}=V^{+}*(1/(1+(s/BWP)))
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
V_{IN}-Zg*(V^{+}-V\#)*s*Cg-V^{+}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-V_{OUT}+(V^{+}-V\#)*Cg+\frac{(V^{-}-V\#)}{s*C*Rg}=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
-V_{OUT}+V\#*(\frac{1}{(s*C*R+1)})=0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{(C*Cg*Rg*s^{2}+BWP*C*Cg*Rg*s+BWP)}{DEN1+DEN2}
\]

\end_inset


\end_layout

\begin_layout Standard
DEN1=
\begin_inset Formula $(C*Cg*(R*Zg+Rg*Zg+C*R*Rg))*s^{3}+(C*R+C*Rg+Cg*Zg+C*Cg*Rg+BWP*C(Cg*Rg*(C*R+Zg)))*s^{2}$
\end_inset


\end_layout

\begin_layout Standard
DEN2=
\begin_inset Formula $(BWP*C*(R+Rg+Cg*Rg)+1)*s+BWP)$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{DEN1}{BWP}=$
\end_inset


\begin_inset Formula $\frac{(C*Cg*(R*Zg+Rg*Zg+C*R*Rg))}{BWP}*s^{3}+(\frac{C*R+C*Rg+Cg*Zg+C*Cg*Rg}{BWP}+C(Cg*Rg*(C*R+Zg)))*s^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{DEN2}{BWP}=(C*(R+Rg+Cg*Rg)+\frac{1}{BWP})*s+1)\approx(C*(R+Rg+Cg*Rg))*s+1)\approx C*(R+Rg)*s+1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{C*Cg*Rg*s+1}{\frac{DEN1}{BWP}+C*(R+Rg)*s+1}
\]

\end_inset


\end_layout

\begin_layout Standard
R*C << Zg;
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{DEN1}{BWP}\approx$
\end_inset


\begin_inset Formula $\frac{(C*Cg*(R*Zg+Rg*Zg+C*R*Rg))}{BWP}*s^{3}+(\frac{C*R+C*Rg+Cg*Zg+C*Cg*Rg}{BWP}+C(Cg*Rg*Zg))*s^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $C*Cg*Rg*s<<1$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{1}{\frac{DEN1}{BWP}+C*(R+Rg)*s+1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $R*Zg+Rg*Zg>>C*R*Rg$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $C*R+C*Rg+Cg*Zg>>C*Cg*Rg$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{DEN1}{BWP}\approx$
\end_inset


\begin_inset Formula $\frac{(C*Cg*(R+Rg)*Zg))}{BWP}*s^{3}+(\frac{C*(R+Rg)+Cg*Zg}{BWP}+C(Cg*Rg*Zg))*s^{2}$
\end_inset


\end_layout

\begin_layout Standard
R>>Rg
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{DEN1}{BWP}\approx$
\end_inset


\begin_inset Formula $\frac{(C*Cg*R*Zg))}{BWP}*s^{3}+(\frac{C*R+Cg*Zg}{BWP}+C(Cg*Rg*Zg))*s^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{DEN2}{BWP}\approx C*R*s+1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{1}{\frac{(C*Cg*R*Zg))}{BWP}*s^{3}+(\frac{C*R+Cg*Zg}{BWP}+C(Cg*Rg*Zg))*s^{2}+C*R*s+1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $C*R<<Cg*Zg$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{1}{\frac{(C*Cg*R*Zg))}{BWP}*s^{3}+((\frac{Cg*Zg}{BWP}+C*Rg*Cg*Zg))*s^{2}+C*R*s+1}
\]

\end_inset


\end_layout

\begin_layout Standard
Despreciamos 
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{(C*Cg*R*Zg))}{BWP}*s^{3}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{1}{C*Rg*Cg*Zg*s^{2}+C*R*s+1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{1}{C*L*s^{2}+C*R*s+1}
\]

\end_inset


\end_layout

\begin_layout Standard
f
\begin_inset Formula $_{0}$
\end_inset

= 11000Hz
\end_layout

\begin_layout Standard

\series bold
Recordamos las aproximaciones hechas hasta ahora:
\end_layout

\begin_layout Standard
R*C << Zg; 
\end_layout

\begin_layout Standard

\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Circuito de un filtro band pass elemental 
\begin_inset CommandInset label
LatexCommand label
name "fig:BP"

\end_inset


\end_layout

\end_inset


\end_layout

\end_inset


\end_layout

\begin_layout Standard
\paragraph_spacing onehalf
\noindent
\begin_inset Formula 
\[
H(s)=H_{0}\frac{\frac{s}{\sqrt{\frac{1}{LC}}}}{\left(\frac{s}{\frac{1}{\sqrt{LC}}}\right)^{2}+RCs+1}
\]

\end_inset


\end_layout

\begin_layout Subsubsection*
Band pass con gyrator:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{Cg*Rg*Zg*s^{2}+(Rg+BWP*Cg*Rg*Zg)*s+BWP*Rg}{DEN1+DEN2}
\]

\end_inset


\end_layout

\begin_layout Standard
DEN1 = 
\begin_inset Formula $C*Cg*R*Rg*Zg*s^{3}+(C*R*Rg+Cg*(R*Rg+(R+Rg)*Zg)+BWP*C*Cg*R*Rg*Zg)*s^{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $DEN2=(R+Rg+BWP*(C*R*Rg+Cg*Rg*(R+Zg)))*s+BWP*R+BWP*Rg$
\end_inset

 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{\frac{Cg*Rg*Zg}{BWP}*s^{2}+(\frac{Rg}{BWP}+Cg*Rg*Zg)*s+Rg}{\frac{DEN1+DEN2}{BWP}}\approx\frac{\frac{Cg*Rg*Zg}{BWP}*s^{2}+(Cg*Rg*Zg)*s+Rg}{\frac{DEN1+DEN2}{BWP}}
\]

\end_inset


\end_layout

\begin_layout Standard
Despreciamos 
\begin_inset Formula $\frac{Rg}{BWP}$
\end_inset


\end_layout

\begin_layout Standard
Para cierto rango de frecuencias 
\begin_inset Formula $\frac{Cg*Rg*Zg}{BWP}$
\end_inset

 sera totalmente despreciable por lo que H(s) nos queda:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{(Cg*Rg*Zg)*s+Rg}{\frac{DEN1+DEN2}{BWP}}
\]

\end_inset


\end_layout

\begin_layout Standard
Y si 
\begin_inset Formula $(Cg*Rg*Zg)*s>>Rg$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{(Cg*Rg*Zg)*s}{\frac{DEN1+DEN2}{BWP}}
\]

\end_inset


\end_layout

\begin_layout Standard
Analicemos 
\begin_inset Formula $\frac{DEN1+DEN2}{BWP}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{DEN1}{BWP}=\frac{C*Cg*R*Rg*Zg}{BWP}*s^{3}+(\frac{C*R*Rg+Cg*(R*Rg+(R+Rg)*Zg)}{BWP}+C*Cg*R*Rg*Zg)*s^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
Despreciamos 
\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{C*Cg*R*Rg*Zg}{BWP}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{DEN1}{BWP}\approx C*Cg*R*Rg*Zg*s^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{DEN2}{BWP}=(\frac{R+Rg}{BWP}+(C*R*Rg+Cg*Rg*(R+Zg)))*s+R+Rg
\]

\end_inset


\end_layout

\begin_layout Standard
Despreciamos 
\begin_inset Formula $\frac{R+Rg}{BWP}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{DEN2}{BWP}\approx((C*R*Rg+Cg*Rg*(R+Zg)))*s+R+Rg
\]

\end_inset


\end_layout

\begin_layout Standard
Zg >> R
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\frac{DEN2}{BWP}\approx((C*R*Rg+Cg*Rg*Zg)))*s+R+Rg
\]

\end_inset


\end_layout

\begin_layout Standard
Finalmente nos queda
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{(Cg*Rg*Zg)*s}{C*Cg*R*Rg*Zg*s^{2}+(C*R*Rg+Cg*Rg*Zg)*s+R+Rg}
\]

\end_inset


\end_layout

\begin_layout Standard
Si R >>Rg
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{(Cg*Rg*Zg)*s}{C*Cg*R*Rg*Zg*s^{2}+(Rg(C*R+Cg*Zg)))*s+R}
\]

\end_inset


\end_layout

\begin_layout Standard
Si L = Cg Rg Zg
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\frac{L*s}{C*L*R*s^{2}+(Rg*C*R+L)*s+R}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\left(\frac{1}{RC}\right)\frac{LC*s}{LC*s^{2}+\frac{(Rg*C*R+L))}{R}*s+1}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H(s)=\left(\frac{1}{R}\sqrt{\frac{L}{C}}\right)\frac{\sqrt{LC}s}{LC*s^{2}+\frac{(Rg*C*R+L))}{R}*s+1}
\]

\end_inset


\end_layout

\begin_layout Standard
Fijamos C en 5.6 nf
\end_layout

\begin_layout Standard
//R en 1K
\end_layout

\begin_layout Standard
si f
\begin_inset Formula $_{0}=14KHz$
\end_inset


\end_layout

\begin_layout Standard
entones L 
\begin_inset Formula $\approx$
\end_inset

 0.024Hy
\end_layout

\begin_layout Standard
Por lo tanto 
\begin_inset Formula $\sqrt{\frac{L}{C}}=2070,2\Omega$
\end_inset


\end_layout

\begin_layout Standard
Si quiero que 
\end_layout

\begin_layout Standard
\begin_inset Formula $\left(\frac{1}{R}\sqrt{\frac{L}{C}}\right)\approx1$
\end_inset

 entonces podría elegir por ejemplo R = 1000 y tendría que 
\begin_inset Formula $\left(\frac{1}{R}\sqrt{\frac{L}{C}}\right)=0.5$
\end_inset


\end_layout

\begin_layout Standard
Si pido 
\begin_inset Formula $\xi>0.707$
\end_inset

 y dado que 
\end_layout

\begin_layout Standard
2
\begin_inset Formula $\frac{\xi}{\omega_{0}}=\frac{(Rg*C*R+L))}{R}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{Rg*C*R+L}{R}*(\frac{2\pi f_{0}}{2})>0.707$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{L}{R}*(\frac{2\pi f_{0}}{2})>0.707$
\end_inset


\end_layout

\begin_layout Standard
1,05547 > 0.707
\end_layout

\begin_layout Standard
Notar que si se hubiese elegido R= 2K quedaría 
\begin_inset Formula $\xi=0.5277$
\end_inset

 lo cual es algo que no deseamos.
\end_layout


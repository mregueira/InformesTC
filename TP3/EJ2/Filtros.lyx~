\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Diferencias entre el teórico completo y el teórico con aproximaciones
\begin_inset CommandInset label
LatexCommand label
name "fig:Diferencias"

\end_inset


\end_layout

\end_inset


\end_layout


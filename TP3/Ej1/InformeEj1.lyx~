\end_layout

\begin_layout Plain Layout

H(S)=(1+
\backslash
frac{R_4}{R_5})(
\backslash
frac{S^2+S
\backslash
frac{(R_5R_6R_8+R_4R_6R_8-R_4R_5R_7)}{C_6R_6R_7R_8(R_4+R_5)}+
\backslash
frac{R_4R_5}{C_2C_6R_1R_3R_8(R_4+R_5)}}{S^2+S(
\backslash
frac{R_6+R_7}{C_6R_6R_7})+
\backslash
frac{R_4(R_5+R_8)}{C_2C_6R_1R_3R_5R_8}})
\end_layout

\begin_layout Plain Layout


\backslash
end{equation}
\end_layout

\end_inset


\end_layout

\begin_layout Standard
